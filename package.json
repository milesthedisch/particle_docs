{
  "name": "particle_docs",
  "version": "1.0.0",
  "description": "API and Documentation.",
  "main": "server.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "setup": "./bin/setup && node ./bin/writeConfigFiles.js",
    "style": "node-sass --recursive --output ./public/build/css --source-map true --source-map-contents ./styles && npm run style:clean",
    "style:clean": "cleancss -o ./public/build/css/main.css ./public/build/css/main.css",
    "style:watch": "node-sass --recursive --watch --output ./public/build/css --source-map true --source-map-contents ./styles && npm run style:clean",
    "serve": "npm run setup && nodemon -e js,hbs,json, --watch ./controllers --watch ./views server.js | bunyan",
    "start": "npm run setup && node server.js",
    "dev": "webpack --config=./webpack.dev.js --bail && npm run style && npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milesthedisch/particle_docs.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/milesthedisch/particle_docs/issues"
  },
  "homepage": "https://github.com/milesthedisch/particle_docs#readme",
  "dependencies": {
    "bunyan": "^1.8.5",
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "extend": "^3.0.0",
    "handlebars": "^4.0.6",
    "particle_library": "git+https://github.com/milesthedisch/particle_library.git"
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "clean-css": "^3.4.21",
    "eslint": "^3.11.0",
    "eslint-config-google": "^0.7.1",
    "html-webpack-plugin": "^2.24.1",
    "node-sass": "^3.13.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
